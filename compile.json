{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "targets": [
    {
      "type": "source",
      "outputPath": "source-output"
    },
    {
      "type": "hybrid",
      "outputPath": "hybrid-output"
    },
    {
      "type": "build",
      "outputPath": "build-output"
    }
  ],
  "defaultTarget": "source",
  "locales": ["en","de"],
  "environment": { 
      "qx.icontheme": "Tango"
  },     
  "applications": [
    {
      "class": "smart.model.TreeTable",
      "theme": "qx.theme.Modern",
      "name": "smart",
      "writeIndexHtmlToRoot": true
    }
  ],
  "libraries": [
    "../qooxdoo",
    "."
  ]
}
